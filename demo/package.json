{
  "name": "jsoo-css-demo",
  "version": "0.1.0",
  "description": "Demo for jsoo-css",
  "license": "MIT",
  "homepage": "https://github.com/ml-in-barcelona/jsoo-css",
  "bugs": {
    "url": "https://github.com/ml-in-barcelona/jsoo-css/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ml-in-barcelona/jsoo-css.git"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "esbuild": "^0.9.2",
    "live-server": "^1.2.1"
  },
  "scripts": {
    "bundle": "node build.mjs",
    "bundle:watch": "yarn bundle --watch --color=true",
    "dev-server": "PORT=7000 live-server . --host=localhost",
    "start": "concurrently --names www,js --kill-others yarn:dev-server yarn:bundle:watch",
    "build": "yarn bundle --minify --define:process.env.NODE_ENV='\"production\"'",
    "clean": "rm -rf dist"
  },
  "browser": {
    "fs": false,
    "tty": false,
    "constants": false,
    "child_process": false
  }
}
