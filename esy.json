    "@opam/jsoo-react": "*"
  },
  "resolutions": {
    "@opam/jsoo-react": "ml-in-barcelona/jsoo-react:jsoo-react.opam#9e86c9f",
    "@reason-native/rely": "jfrolich/reason-native:rely.json#08085a4048974b06aaf3aab918b677b39dea3d18",
